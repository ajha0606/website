blogdown::install_hugo()
install.packages("blogdown")
blogdown::install_hugo()
blogdown::new_site(theme="nurlansu/hugo-sustain", theme_example = T)
blogdown::serve_site()
country_profile_variables <- read.csv("~/Desktop/country_profile_variables.csv", header=FALSE)
View(country_profile_variables)
happiness <- read.csv("~/Desktop/2017.csv")
View(happiness)
knitr::opts_chunk$set(echo = TRUE)
hap <- pivot_wider(`happiness`, names_from = Happiness.Score, values_from = Country)
library(tidyr)
library(dplyr)
library(ggplot2)
hap <- pivot_wider(`happiness`, names_from = Happiness.Score, values_from = Country)
happy <- hap %>% pivot_longer(11:161, names_to = "happinesss", values_to = "country") %>% na.omit
chiao <- country_profile_variables %>% mutate(country=V1, region=V2, population_thousands=V4, GDP=V7, unemployment.percent=V16, life.expect=V28) %>% select(51:56) %>% slice(-1)
hola <- pivot_wider(chiao, names_from = "region", values_from = "country")
country <- hola %>% pivot_longer(5:26, names_to = "region", values_to = "country") %>% na.omit
join <- happy %>% inner_join(country, by="country")
head(join)
join <- join %>% select(-Whisker.high, -Whisker.low)
join <- join %>% separate(life.expect, into=c("life_fem", "life_male"), sep = "/")
blogdown::serve_site()
